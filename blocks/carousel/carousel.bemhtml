block carousel {

    js: true

    content: [
        {
            elem: 'inner',
            content: this.ctx.content
        },
        {
            elem: 'control',
            elemMods: { slide: 'left' },
            content: '‹'
        },
        {
            elem: 'control',
            elemMods: { slide: 'right' },
            content: '›'    
        }
    ]
}
